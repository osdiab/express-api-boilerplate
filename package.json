{
  "name": "express-api-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for writing pretty Express-based APIs",
  "license": "MIT",
  "scripts": {
    "dev-server": "nodemon --exec \"babel-node --inspect src\"",
    "build": "babel -d ./build src",
    "start": "npm run-script build && nodemon --exec \"NODE_ENV=production node build/index.js\"",
    "test": "jest",
    "lint": "eslint 'src/**/*.js'",
    "fix-lint": "prettier --write './src/**/*.js' && npm run-script lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osdiab/gymi-website-node.git"
  },
  "keywords": [
    "frontend",
    "boilerplate"
  ],
  "author": "Omar Diab (github/osdiab) and Rona Wo (github/YanrongWo)",
  "bugs": {
    "url": "https://github.com/osdiab/gymi-website-node/issues"
  },
  "homepage": "https://github.com/osdiab/gymi-website-node#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-force-ssl": "^0.3.2",
    "express-request-language": "^1.1.15",
    "jsonwebtoken": "^8.3.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2"
  }
}
